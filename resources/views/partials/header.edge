      <li class="flex">
        <form
          method="get"
          action="{{ route('products') }}"
          id="search"
          class="flex items-center font-medium relative"
        >
          <input
            type="text"
            name="search"
            id="searchInput"
            placeholder="Search Products..."
            class="bg-gray-50 rounded-lg pl-2 pr-8 py-1 w-0 opacity-0 focus:outline-none focus:ring-2 focus:ring-blue-500 transition-all duration-300"
            value="{{ request.input('search') ?? '' }}"
            disabled
          />
          <button
            type="button"
            id="toggleSearch"
            class="text-gray-50 cursor-pointer absolute right-1 hover:-translate-y-0.5 transition-all duration-300"
            aria-label="Toggle Search Button"
          >
