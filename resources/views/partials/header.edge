      @each(item in navbarItems)
        <li class="">
          <a
            href="{{ route(item) }}"
            class="{{ request.matchesRoute(item) ? 'font-bold border-b-gray-100' : 'hover:border-b-gray-100 hover:-translate-y-0.5' }} text-gray-50 p-1 block transition-all duration-300 border-b-2 border-transparent"
